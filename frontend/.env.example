# VITE Environment Variables for Frontend
# These must start with VITE_ prefix to be accessible in React

# API Base URL - Change this to your Vercel deployment URL
VITE_API_URL=https://your-project-name.vercel.app/api

# App Information
VITE_APP_NAME=EXPENZO
VITE_APP_VERSION=1.0.0

# Optional: Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_OCR=true
VITE_ENABLE_BANK_IMPORT=true
